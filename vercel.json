{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "http://127.0.0.1:8000/api",
    "REACT_APP_FACEBOOK_APP_ID": "${FACEBOOK_APP_ID}",
    "REACT_APP_FACEBOOK_PAGE_ID": "${FACEBOOK_PAGE_ID}"
  }
}
